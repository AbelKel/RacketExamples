(sqr 4)
(max 10 15)
(cos 0)

(sqrt 1)
(sqrt -1)


(define (area-of-circle r)
  (* pi (sqr r)))


(area-of-circle 5)

(define (area-of-ring outer inner)
  (- (area-of-circle outer)
     (area-of-circle inner)))


(area-of-ring 5 3)

